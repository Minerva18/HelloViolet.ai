{
  "source": {
    "include": [
      "../violet-conversations/lib",
      "../violet-samples/tutorials",
      "../violet-samples/scripts",
      "../violet-samples/apps"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["jsdoc/custom-tags.js"],
  "tags": {
    "allowUnknownTags": false
  },
  "templates": {
    "default": {
      "layoutFile": "jsdoc/main.tmpl"
    }
  },
  "opts": {
    "destination": "docs"
  }
}
